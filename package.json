{
  "name": "kaminari-core",
  "version": "0.1.0",
  "description": "Client for Kaminari blockchain platform",
  "author": "shuse2",
  "license": "Apache-2.0",
  "keywords": [
    "cryptocurrency",
    "blockchain",
    "lisk",
    "smart contract",
    "javascript"
  ],
  "homepage": "https://github.com/xdgetech/kaminari-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdgetech/kaminari-core.git"
  },
  "bugs": {
    "url": "https://github.com/xdgetech/kaminari-core/issues"
  },
  "engines": {
    "node": ">=12.18.3",
    "npm": ">=6.14.4"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "format": "prettier --write '**/*'",
    "prepack": "oclif-dev manifest && npm shrinkwrap && oclif-dev readme --multi --dir=docs/commands",
    "postpack": "rm -f oclif.manifest.json",
    "prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo",
    "build": "tsc",
    "test": "mocha test --exclude 'test/scripts/**'",
    "test:stress": "npx ts-node test/scripts/stress_test.ts",
    "version": "oclif-dev readme --multi --dir=docs/commands && git add README.md docs",
    "prepublishOnly": "rm -r ./node_modules && npm install && npm run lint && npm run build"
  },
  "bin": {
    "kaminari-core": "./bin/run"
  },
  "lisk": {
    "addressPrefix": "lsk"
  },
  "oclif": {
    "bin": "kaminari-core",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/dist",
    "/config",
    "/docs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "3.1.0",
    "fs-extra": "9.0.1",
    "lisk-sdk": "5.0.0-alpha.3",
    "tslib": "1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/debug": "4.1.5",
    "@types/ed2curve": "0.2.2",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/jest": "26.0.3",
    "@types/jest-when": "2.7.1",
    "@types/node": "12.12.11",
    "@types/semver": "7.3.3",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "copyfiles": "2.2.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-lisk-base": "1.2.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-extended": "0.11.5",
    "jest-when": "2.7.2",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "source-map-support": "0.5.19",
    "ts-jest": "26.1.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  }
}
